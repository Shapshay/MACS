<tr id="tr_{BACK_ID}">
    <td>{BACK_DATE}</td>
    <td>{BACK_FILE}</td>
    <td>
        <a href="{BACK_URL}" title="Скачать" target="_blank"><img src="images/download.png" alt="Скачать" /></a>
    </td>
    <td>
        <a href="javascript:BackRestore({BACK_ID}, '{BACK_DATE}');" title="Востановить" target="_blank"><img src="images/hammer_screwdriver.png" alt="Востановить" /></a>
    </td>
    <td>
        <a href="javascript:DelBack({BACK_ID}, '{BACK_DATE}', 'tr_{BACK_ID}');" title="Delete"><img src="images/cross.png" alt="Delete" /></a>
    </td>
</tr>